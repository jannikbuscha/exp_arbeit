    "@nx/js": "20.3.2",
    "@nx/module-federation": "20.3.2",
    "@nx/web": "20.3.2",
    "@nx/webpack": "20.3.2",
    "@nx/workspace": "20.3.2",
    "@schematics/angular": "~19.0.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
