  "license": "MIT",
  "scripts": {
    "shell:build:prod:stats": "nx build shell --prod --skip-nx-cache -- --statsJson",
    "shell:build:dev:stats": "nx build shell --skip-nx-cache -- --statsJson",
    "analyze:prod:stats": "run-s \"shell:build:prod:stats\" \"wba-shell\"",
    "analyze:dev:stats": "run-s \"shell:build:dev:stats\" \"wba-shell\"",
    "mfe1:build:prod:stats": "nx build mfe1 --prod --skip-nx-cache -- --statsJson",
    "mfe1:build:dev:stats": "nx build mfe1 --skip-nx-cache -- --statsJson",
    "analyze-mfe1:prod:stats": "run-s \"wba-mfe1\"",
    "analyze-mfe1:dev:stats": "run-s \"wba-mfe1\"",
    "mfe2:build:prod:stats": "nx build mfe2 --prod --skip-nx-cache -- --statsJson",
    "mfe2:build:dev:stats": "nx build mfe2 --skip-nx-cache -- --statsJson",
    "analyze-mfe2:prod:stats": "run-s \"wba-mfe2\"",
    "analyze-mfe2:dev:stats": "run-s \"wba-mfe2\"",
    "wba-shell": "webpack-bundle-analyzer dist/apps/shell/stats.json",
    "wba-mfe1": "webpack-bundle-analyzer dist/apps/mfe1/stats.json",
    "wba-mfe2": "webpack-bundle-analyzer dist/apps/mfe2/stats.json"
  },
  "private": true,
  "devDependencies": {
